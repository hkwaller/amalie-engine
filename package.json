{
  "name": "amalie-engine",
  "version": "0.1.1",
  "description": "A TypeScript quiz game engine with Supabase Realtime integration, React hooks, configurable scoring, and power-up support",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/hkwaller/amalie-engine.git"
  },
  "homepage": "https://github.com/hkwaller/amalie-engine#readme",
  "bugs": {
    "url": "https://github.com/hkwaller/amalie-engine/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.mjs",
      "require": "./dist/components/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "quiz",
    "game",
    "trivia",
    "supabase",
    "realtime",
    "react",
    "hooks",
    "multiplayer"
  ],
  "author": "hkwaller",
  "license": "MIT",
  "peerDependencies": {
    "@supabase/supabase-js": "^2.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "qrcode.react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@supabase/supabase-js": "^2.47.0",
    "@types/node": "^22.10.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0"
  },
  "optionalDependencies": {
    "qrcode.react": "^4.1.0"
  }
}
